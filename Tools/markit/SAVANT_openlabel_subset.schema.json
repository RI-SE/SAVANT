{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OpenLabel SAVANT Subset",
  "type": "object",
  "properties": {
    "openlabel": {
      "type": "object",
      "properties": {
        "metadata": {
          "type": "object",
          "properties": {
            "schema_version": { "type": "string"},
            "tagged_file": { "type": "string" },
            "annotator": { "type": "string" }
          },
          "required": ["schema_version", "tagged_file", "annotator"]
        },
        "ontologies": {
          "type": "object",
          "patternProperties": {
            "^[0-9]+$": { "type": "string", "format": "uri" }
          }
        },
        "objects": {
          "type": "object",
          "patternProperties": {
            "^[0-9]+$": {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "type": { "type": "string" },
                "ontology_uid": { "type": ["string", "number"] },
                "frame_intervals": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "frame_start": { "type": "integer" },
                      "frame_end": { "type": "integer" }
                    },
                    "required": ["frame_start", "frame_end"]
                  }
                }
              },
              "required": ["name", "type", "ontology_uid"]
            }
          }
        },
        "actions": {
          "type": "object",
          "patternProperties": {
            "^[0-9]+$": {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "type": { "type": "string" },
                "ontology_uid": { "type": ["string", "number"] },
                "frame_intervals": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "frame_start": { "type": "integer" },
                      "frame_end": { "type": "integer" }
                    },
                    "required": ["frame_start", "frame_end"]
                  }
                }
              },
              "required": ["name", "type", "ontology_uid"]
            }
          }
        },
        "frames": {
          "type": "object",
          "patternProperties": {
            "^[0-9]+$": {
              "type": "object",
              "properties": {
                "objects": {
                  "type": "object",
                  "patternProperties": {
                    "^[0-9]+$": {
                      "type": "object",
                      "properties": {
                        "object_data": {
                          "type": "object",
                          "properties": {
                            "rbbox": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "name": { "type": "string" },
                                  "val": {
                                    "type": "array",
                                    "items": { "type": ["number", "string"] }
                                  }
                                },
                                "required": ["name", "val"]
                              }
                            },
                            "vec": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "name": { "type": "string" },
                                  "val": {
                                    "type": "array",
                                    "items": { "type": ["number", "string"] }
                                  }
                                },
                                "required": ["name", "val"]
                              }
                            }
                          },
                          "required": ["rbbox", "vec"]
                        }
                      },
                      "required": ["object_data"]
                    }
                  }
                }
              },
              "required": ["objects"]
            }
          }
        }
      },
      "required": ["metadata", "ontologies", "objects", "actions", "frames"]
    }
  },
  "required": ["openlabel"]
}